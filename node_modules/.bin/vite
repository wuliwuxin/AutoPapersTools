#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/vite@7.1.9_@types+node@24.7.0_jiti@2.6.1_lightningcss@1.30.1_tsx@4.20.6/node_modules/vite/bin/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/vite@7.1.9_@types+node@24.7.0_jiti@2.6.1_lightningcss@1.30.1_tsx@4.20.6/node_modules/vite/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/vite@7.1.9_@types+node@24.7.0_jiti@2.6.1_lightningcss@1.30.1_tsx@4.20.6/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/vite@7.1.9_@types+node@24.7.0_jiti@2.6.1_lightningcss@1.30.1_tsx@4.20.6/node_modules/vite/bin/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/vite@7.1.9_@types+node@24.7.0_jiti@2.6.1_lightningcss@1.30.1_tsx@4.20.6/node_modules/vite/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/vite@7.1.9_@types+node@24.7.0_jiti@2.6.1_lightningcss@1.30.1_tsx@4.20.6/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
