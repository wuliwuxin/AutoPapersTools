#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules/@vercel/nft/out/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules/@vercel/nft/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules/@vercel/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules/@vercel/nft/out/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules/@vercel/nft/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules/@vercel/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/@vercel+nft@1.1.1_rollup@4.52.4/node_modules:/Users/xinwu/iCloud云盘（归档）/Documents/github_project/AutoPapersTools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/nft/out/cli.js" "$@"
else
  exec node  "$basedir/../@vercel/nft/out/cli.js" "$@"
fi
